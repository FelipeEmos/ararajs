{
  "name": "arara",
  "version": "0.0.1",
  "private": false,
  "description": "Unstyled, accessible and customizable UI primitives for SolidJS",
  "keywords": [
    "solid",
    "solidjs",
    "headless",
    "accessible",
    "customizable",
    "primitives"
  ],
  "homepage": "https://arara-ten.vercel.app",
  "bugs": {
    "url": "https://github.com/araradev/arara/issues",
    "email": "code@jasi.dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/araradev/arara.git"
  },
  "license": "MIT",
  "author": {
    "name": "Felipe Emos",
    "email": "felipe.emos.computacao@gmail.com",
    "url": "https://github.com/FelipeEmos"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    "./*": {
      "types": "./dist/*.d.ts",
      "solid": "./dist/*.jsx",
      "default": "./dist/*.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf .turbo dist node_modules",
    "dev": "tsup --watch",
    "lint": "eslint --max-warnings=0 ."
  },
  "dependencies": {
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^8.9.0",
    "esbuild-plugin-solid": "^0.6.0",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-solid": "^0.14.3",
    "prettier": "^3.3.3",
    "solid-js": "^1.9.2",
    "tsup": "^8.3.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.9.0"
  },
  "peerDependencies": {
    "solid-js": "^1.8"
  }
}
