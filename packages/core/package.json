{
  "name": "@ararajs/core",
  "version": "0.0.1",
  "private": false,
  "description": "Animation primitives for procedural and physics animations on SolidJS",
  "keywords": [
    "solid",
    "solidjs",
    "animation",
    "procedural",
    "physics",
    "primitives"
  ],
  "homepage": "https://ararajs.vercel.app/",
  "bugs": {
    "url": "https://github.com/FelipeEmos/arara/issues",
    "email": "felipe.emos.computacao@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FelipeEmos/arara.git"
  },
  "license": "MIT",
  "author": {
    "name": "Felipe Emos",
    "email": "felipe.emos.computacao@gmail.com",
    "url": "https://github.com/FelipeEmos"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "solid": "./dist/index.jsx",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "solid": "./dist/*.jsx",
      "default": "./dist/*.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf .turbo dist node_modules",
    "dev": "tsup --watch",
    "lint": "eslint --max-warnings=0 .",
    "lint:fix": "eslint --fix 'src/**/*.{js,cjs,mjs,ts,tsx}'"
  },
  "dependencies": {
    "gl-matrix": "^3.4.3"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^8.9.0",
    "esbuild-plugin-solid": "^0.6.0",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-solid": "^0.14.3",
    "prettier": "^3.3.3",
    "solid-js": "^1.9.2",
    "tsup": "^8.3.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.9.0"
  },
  "peerDependencies": {
    "solid-js": "^1.8"
  }
}
